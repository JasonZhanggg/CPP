#include <iostream>
#include <cstring>
using namespace std;
int main(){
  char input[80];
  char newInput[80];
  cin.getline(input, sizeof(input));
  for(int i = 0, k = 0; i<sizeof(input); i++){
    if(!(ispunct(input[i]) || input[i] == ' ' || input[i] == '"')){
      newInput[k] = tolower(input[i]);
      k++;
    }
  }
  for(int i = 0, k = strlen(newInput)-1; i < k ; i++, k--){
    if(newInput[i] != newInput[k]){
      cout << "Not Palindrome";
      return 0;
    }
    cout << "Palindrome";
    return 0;
  }
}
