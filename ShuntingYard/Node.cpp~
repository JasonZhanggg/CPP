#include <iostream>
#include <cstring>
#include "Node.h"

using namespace std;

//Constructor
Node::Node(char* newVal) {
  val = newVal;
  next = NULL;
  right = NULL;
  left = NULL;
}

char* Node:: getVal(){
  return val;
}
void Node::setNext(char* newNext){
  next = newNext;
}

Node* Node::getNext(){
  return next;
}
void Node::setRight(char* newRight){
  right = newRight;
}
char* Node::getRight(){
  return right;
}
void Node::setLeft(char* newLeft){
  left = newLeft;
}
char* Node::getLeft(){
  return left;
}

Node::~Node(){
  delete val;
  next = NULL;
}
